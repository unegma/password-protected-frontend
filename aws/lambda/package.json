{
  "name": "lambda",
  "version": "1.79.0",
  "main": "index.js",
  "scripts": {
    "install-deps": "npm install;",
    "update-install": "npm update; npm install;",
    "compile": "npx tsc",
    "compile-watch": "npx tsc -w",
    "compile-on-fly": "ts-node ./src/index.ts",
    "### Deploy Functions": "",
    "BUILD": "npm run compile && cp package.json ./dist && (cd ./dist && npm install --only=production && npx node-prune)",
    "CLEANUP": "rm -R ./dist/node_modules && rm ./dist/package.json && rm ./dist/package-lock.json && rm ./dist/function.zip",
    "TAG": "npm version --commit-hooks false --git-tag-version false minor",
    "DEPLOY-INDIVIDUAL": "npm run BUILD && npm run publish-individual && npm run CLEANUP && npm run TAG"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.83",
    "@types/jest": "^27.0.1",
    "@types/mocha": "^9.0.0",
    "@types/node": "^14.15.5",
    "chai": "^4.2.0",
    "esm": "^3.2.25",
    "lambda-tester": "^4.0.1",
    "mocha": "^8.1.3",
    "node-prune": "^1.0.2",
    "proxyquire": "^2.1.3",
    "sinon": "^9.2.1",
    "sinon-chai": "^3.5.0",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "@unegma/aws-utilities": "^2.0.0",
    "@unegma/logger": "^3.1.4",
    "@unegma/utilities": "^1.3.2",
    "aws-sdk": "^2.780.0",
    "axios": "^0.21.4",
    "dotenv": "^8.2.0",
    "googleapis": "^105.0.0",
    "node-fetch": "^2.6.1"
  }
}
